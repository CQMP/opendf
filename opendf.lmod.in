#%Module1.0

set     name        opendf
set     root        @CMAKE_INSTALL_PREFIX@


setenv      OPENDF_DIR    $root
setenv      OPENDF_ROOT   $root
setenv      OPENDF_LIB    $root/lib
setenv      OPENDF_INC    $root/include
setenv      OPENDF_BIN    $root/bin

prepend-path      PATH      $root/bin
prepend-path      LD_LIBRARY_PATH $root/lib
prepend-path      PYTHONPATH $root/lib/python
